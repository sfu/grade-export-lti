<% provide(:title, @user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= @user.name %>
      </h1>
      <div>
        <ul>
          <li><%= @user.id %></li>
          <li><%= @user.email %></li>
          <li><%= logged_in? %></li>
          <li>Session id: <%= session[:user_id] %></li>
          <li>Access Token: <%= current_user.access_token %></li>
        </ul>
      </div>
      <a href="#" class="btn btn-primary" role="button">Get Started</a>
    </section>
  </aside>
</div>